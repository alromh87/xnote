<?xml version='1.0' encoding='UTF-8'?>

<RDF xmlns='http://www.w3.org/1999/02/22-rdf-syntax-ns#'
     xmlns:em='http://www.mozilla.org/2004/em-rdf#'>

  <Description about='urn:mozilla:install-manifest'>
    <em:id>xnote@froihofer.net</em:id>
    <em:version>2.2.1</em:version>
    <em:type>2</em:type> <!-- 2 corresponds to type 'extension' -->
    <em:locale>locale/en-US/</em:locale> 
    <!--
    <em:optionsURL>chrome://xnote/content/preferences.xul</em:optionsURL>
    -->
    <em:targetApplication>
      <Description>
        <!-- id of Thunderbird -->
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>3.1.*</em:maxVersion>
      </Description>
    </em:targetApplication>
	
    <!-- Visible meta-data -->
    <em:name>XNote</em:name>
    <em:description>Add Post-It to your mails</em:description>
    <em:iconURL>chrome://xnote/skin/icon32.png</em:iconURL>

    <!--
    <em:description>Création et association de notes aux mails sous forme de Post-It</em:description>
    -->
    <em:creator>Lorenz Froihofer: TB3 port, since Dec. 2009</em:creator>
    <em:contributor>Hugo Smadja: original developer</em:contributor>
    <em:contributor>Pierre-André Galmes: TB2 port, till Apr. 2008</em:contributor>
    <em:translator>Maarten van Vliet: nl-NL</em:translator>
    <em:translator>Björn Milcke: de-DE, en-US</em:translator>
    <em:translator>Julien Buratto: it-IT</em:translator>

    <em:homepageURL>http://www.froihofer.net/xnote/</em:homepageURL>
    <!-- 
    <em:updateURL>http://starxpert.fr/outils/XNote/update.rdf</em:updateURL>
    -->
  </Description>
</RDF>
