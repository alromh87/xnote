<?xml version='1.0' encoding='UTF-8'?>

<RDF xmlns='http://www.w3.org/1999/02/22-rdf-syntax-ns#'
     xmlns:em='http://www.mozilla.org/2004/em-rdf#'>

  <Description about='urn:mozilla:install-manifest'>
    <em:id>xnote@starxpert.fr</em:id>
    <em:version>2.0.1</em:version>
	<em:type>2</em:type> <!-- 2 correspond au type 'extension' -->
	<em:locale>locale/fr-FR/</em:locale> 
    <!--
    <em:optionsURL>chrome://xnote/content/preferences.xul</em:optionsURL>
    -->
    <!-- l'application cible de votre extension,
         avec les versions minimums et maximums supportées. -->
    <em:targetApplication>
      <Description>
			<!-- id de Thunderbird -->
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.0.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
	
    <!-- Méta-données visibles -->
    <em:name>XNote</em:name>
    <em:description>Add Post-It to your mails</em:description>
    <!--
    <em:description>Création et association de notes aux mails sous forme de Post-It</em:description>
    -->
    <em:creator>Hugo Smadja</em:creator>
    <em:contributor>Pierre-André Galmes</em:contributor>
    <em:contributor>Björn Milcke</em:contributor>
    <em:contributor>Julien Buratto</em:contributor>

    <em:homepageURL>http://www.starxpert.fr/</em:homepageURL>
    <!-- 
    <em:updateURL>http://starxpert.fr/outils/XNote/update.rdf</em:updateURL>
    -->
  </Description>
</RDF>
